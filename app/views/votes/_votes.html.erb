<table>
  <tr><th>Contestants</th></tr>
  <%= fields_for vote do |v| %>
    <% accounts.each do |a| -%>
      <tr>
        <td>
          <%= v.radio_button :account_id, a.try(:id), checked: (vote.try(:account_id).present?) %> <%= a.try(:first_name) %> <%= a.try(:last_name) %><br>
          <%= %Q{<a href="##{a.try(:id)}" onclick="$('##{a.try(:id)}').removeClass('hidden'); $(this).remove();">show video<br></a>}.html_safe %>

          <%= %Q{<div id="#{a.try(:id)}" class="hidden">}.html_safe %>
            <%= %Q{<iframe width="560" height="315" src="https://www.youtube.com/embed/#{h(a.try(:video_embed_url))}" frameborder="0" allowfullscreen id="embedded_video"></iframe>}.html_safe %><br>
          </div>
        </td>
      </tr>
    <% end %>
  <% end %>
</table>

